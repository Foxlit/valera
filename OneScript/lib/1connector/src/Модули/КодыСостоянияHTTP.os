	Коды.Вставить(426, "Upgrade Required");
	Коды.Вставить(428, "Precondition Required");
	Коды.Вставить(429, "Too Many Requests");
	Коды.Вставить(431, "Request Header Fields Too Large");
	Коды.Вставить(449, "Retry With");
	Коды.Вставить(451, "Unavailable For Legal Reasons");
	Коды.Вставить(499, "Client Closed Request");
	Коды.Вставить(500, "Internal Server Error");
	Коды.Вставить(501, "Not Implemented");
	Коды.Вставить(502, "Bad Gateway");
	Коды.Вставить(503, "Service Unavailable");
	Коды.Вставить(504, "Gateway Timeout");
	Коды.Вставить(505, "HTTP Version Not Supported");
	Коды.Вставить(506, "Variant Also Negotiates");
	Коды.Вставить(507, "Insufficient Storage");
	Коды.Вставить(508, "Loop Detected");
	Коды.Вставить(509, "Bandwidth Limit Exceeded");
	Коды.Вставить(510, "Not Extended");
	Коды.Вставить(511, "Network Authentication Required");
	Коды.Вставить(520, "Unknown Error");
	Коды.Вставить(521, "Web Server Is Down");
	Коды.Вставить(522, "Connection Timed Out");
	Коды.Вставить(523, "Origin Is Unreachable");
	Коды.Вставить(524, "A Timeout Occurred");
	Коды.Вставить(525, "SSL Handshake Failed");
	Коды.Вставить(526, "Invalid SSL Certificate");

	Представление = Коды.Получить(КодСостояния);
	Если Представление = Неопределено Тогда
		Возврат СтрШаблон(НСтр("ru = '%1: Неизвестный код состояния HTTP'"), КодСостояния);
	Иначе
		Возврат СтрШаблон("%1: %2", КодСостояния, Представление);
	КонецЕсли;

	Возврат Коды;

