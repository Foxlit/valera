// Возвращает текстовое представление переданного кода состояния HTTP.
//
// Параметры:
//   КодСостояния - Число - код состояния HTTP, для которого нужно получить текстовое представление.
//
// Возвращаемое значение:
//   Строка - текстовое представление кода состояния HTTP.
//
Функция Представление(КодСостояния) Экспорт

	Коды = Новый Соответствие;
	Коды.Вставить(100, "Continue");
	Коды.Вставить(101, "Switching Protocols");
	Коды.Вставить(102, "Processing");
	Коды.Вставить(103, "Early Hints");
	Коды.Вставить(200, "OK");
	Коды.Вставить(201, "Created");
	Коды.Вставить(202, "Accepted");
	Коды.Вставить(203, "Non-Authoritative Information");
	Коды.Вставить(204, "No Content");
	Коды.Вставить(205, "Reset Content");
	Коды.Вставить(206, "Partial Content");
	Коды.Вставить(207, "Multi-Status");
	Коды.Вставить(208, "Already Reported");
	Коды.Вставить(226, "IM Used");
