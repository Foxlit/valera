	Коды.Вставить(302, "Moved Temporarily");
	Коды.Вставить(303, "See Other");
	Коды.Вставить(304, "Not Modified");
	Коды.Вставить(305, "Use Proxy");
	Коды.Вставить(307, "Temporary Redirect");
	Коды.Вставить(308, "Permanent Redirect");
	Коды.Вставить(400, "Bad Request");
	Коды.Вставить(401, "Unauthorized");
	Коды.Вставить(402, "Payment Required");
	Коды.Вставить(403, "Forbidden");
	Коды.Вставить(404, "Not Found");
	Коды.Вставить(405, "Method Not Allowed");
	Коды.Вставить(406, "Not Acceptable");
	Коды.Вставить(407, "Proxy Authentication Required");
	Коды.Вставить(408, "Request Timeout");
	Коды.Вставить(409, "Conflict");
	Коды.Вставить(410, "Gone");
	Коды.Вставить(411, "Length Required");
	Коды.Вставить(412, "Precondition Failed");
	Коды.Вставить(413, "Payload Too Large");
	Коды.Вставить(414, "URI Too Long");
	Коды.Вставить(415, "Unsupported Media Type");
	Коды.Вставить(416, "Range Not Satisfiable");
	Коды.Вставить(417, "Expectation Failed");
